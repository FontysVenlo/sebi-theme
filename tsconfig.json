{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "moduleResolution": "node",
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "alwaysStrict": true,
      "allowUnreachableCode": false,
      "strictNullChecks": true,
      "noImplicitReturns": true,
      "noUncheckedIndexedAccess": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true
    }
  }